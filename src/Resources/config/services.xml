<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="Vis\VisuallySearchProducts\Snippet\Files\de_DE\SnippetFile_de_DE">
            <tag name="shopware.snippet.file" priority="100"/>
        </service>

        <service id="Vis\VisuallySearchProducts\Snippet\Files\en_GB\SnippetFile_en_GB">
            <tag name="shopware.snippet.file" priority="100"/>
        </service>

        <service id="Vis\VisuallySearchProducts\Storefront\Controller\VisuallySearchController" public="true">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService"/>
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>

    </services>
</container>
