{
  "name": "vis/visually-search-products",
  "description": "Offer your customers the possibility of finding products using single picture!",
  "version": "1.0.0",
  "type": "shopware-platform-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "VisualSearch GmbH",
      "email": "office@visualsearch.at",
      "homepage": "https://www.visualsearch.at"
    }
  ],
  "require": {
    "php": "^7.2",
    "shopware/core": "^6.3",
    "shopware/storefront": "^6.3",
    "shopware/administration": "^6.3"
  },
  "conflict": {
    "shopware/storefront": "<6,>=7",
    "shopware/administration": "<6,>=7"
  },
  "suggest": {
    "shopware/administration": "Allows you to to set up the plugin easily"
  },
  "autoload": {
    "psr-4": {
      "Vis\\VisuallySearchProducts\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vis\\VisuallySearchProducts\\Test\\": "tests/"
    }
  },
  "extra": {
    "shopware-plugin-class": "Vis\\VisuallySearchProducts\\VisVisuallySearchProducts",
    "copyright": "Copyright (c) 2021 VisualSearch GmbH",
    "label": {
      "de-DE": "Visuelle Suche nach Produkten",
      "en-GB": "Visually Search for Products"
    },
    "description": {
      "de-DE": "Visuelle Produktsuche mit Hilfe einer Kamerataste in der Suchleiste.",
      "en-GB": "Visually search products using a camera button in the search bar."
    },
    "manufacturerLink": {
      "de-DE": "https://www.visualsearch.at/",
      "en-GB": "https://www.visualsearch.at/"
    },
    "supportLink": {
      "de-DE": "https://www.visualsearch.at/index.php/support",
      "en-GB": "https://www.visualsearch.at/index.php/support"
    }
  }
}
